{
  "name": "creactivistas-tests",
  "version": "0.9.9",
  "description": "Aplicacion web de Creactivistas, que contiene el test de personalidad Zoom basado en el MBTI, y el test open source Big 5, basado en el modelo de los 5 grandes.",
  "private": true,
  "author": {
    "name": "Alejandro R Buteler",
    "email": "abuteler@enneagonstudios.com",
    "url": "https://github.com/abuteler"
  },
  "contributors": [
    {
      "name": "Open source project",
      "email": "jonas.enge@t-fk.no",
      "url": "https://github.com/telemark"
    }
  ],
  "main": "server.js",
  "engines": {
    "node": ">=12.16.1"
  },
  "scripts": {
    "test": "standard && ava",
    "audit": "npm audit",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "cleanup": "rm -rf .next",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "now && now alias"
  },
  "keywords": [
    "mbti",
    "bigfive",
    "personalidad"
  ],
  "license": "MIT",
  "dependencies": {
    "@alheimsins/b5-johnson-120-ipip-neo-pi-r": "5.0.5",
    "@alheimsins/b5-result-text": "3.0.4",
    "@alheimsins/react-google-charts": "1.6.5",
    "@sendgrid/mail": "^7.2.1",
    "axios": "0.19.2",
    "b5-calculate-score": "1.0.4",
    "babel-eslint": "10.1.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.23.0",
    "iso-639-1": "2.1.3",
    "mongodb": "^3.5.9",
    "next": "9.4.4",
    "next-offline": "5.0.2",
    "next-routes": "1.4.2",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-google-charts": "3.0.15",
    "react-icons": "3.10.0",
    "request-country": "0.1.5"
  },
  "devDependencies": {
    "ava": "3.8.2",
    "coveralls": "3.1.0",
    "husky": "4.2.5",
    "nyc": "15.1.0",
    "standard": "14.3.4"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "ava": {
    "files": [
      "test/**/*"
    ]
  }
}
